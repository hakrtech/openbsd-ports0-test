     then
       AC_CHECK_LIB(cdda_paranoia,
-                   paranoia_cachemodel_size,
+                   paranoia_init,
                    : ,
                    HAVE_CDPARANOIA=no,
-  		 [-lcdda_interface])
+  		 [-lcdda_interface -lm])
     fi
 
     AC_SUBST(CDPARANOIA_LIBS)
