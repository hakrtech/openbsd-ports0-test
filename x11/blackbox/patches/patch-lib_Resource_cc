+void bt::Resource::write(const char* resource, unsigned long long value) {
+  char tmp[64];
+  sprintf(tmp, "%llu", value);
+  write(resource, tmp);
+}
+
+
 void bt::Resource::write(const char* resource, bool value)
 { write(resource, boolAsString(value)); }
 
