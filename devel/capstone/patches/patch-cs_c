-				if (insn->detail->mips.operands[i].type == op_type)
+			for (i = 0; i < insn->detail->mipsen.op_count; i++)
+				if (insn->detail->mipsen.operands[i].type == op_type)
 					count++;
 			break;
 		case CS_ARCH_PPC:
@@ -579,8 +579,8 @@ int cs_op_index(csh ud, cs_insn *insn, unsigned int op
 			}
 			break;
