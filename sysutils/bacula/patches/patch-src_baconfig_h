+++ src/baconfig.h	Mon Nov 14 10:17:54 2011
@@ -607,6 +607,11 @@ int setdomainname(char *name, int namelen);
 #endif /* HAVE_HPUX_OS */
 
 
+#if defined (__OpenBSD__) && defined (__alpha__)
+#undef  ioctl_req_t
+#define ioctl_req_t unsigned long int
+#endif
+
 #ifdef HAVE_OSF1_OS
 #ifdef  __cplusplus
 extern "C" {
